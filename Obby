--// Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

--// T·∫°o GUI
local Window = Rayfield:CreateWindow({
   Name = "üíÄ TUNG HUB GUI",
   LoadingTitle = "ƒêang b·∫≠t GUI...",
   LoadingSubtitle = "B·ªüi ChatGPT x Tung",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "TungHub",
      FileName = "Config"
   }
})

--// N√∫t toggle GUI üíÄ
local toggleBtn = Instance.new("TextButton")
toggleBtn.Parent = game.CoreGui
toggleBtn.Size = UDim2.new(0, 50, 0, 50)
toggleBtn.Position = UDim2.new(0, 10, 0, 10)
toggleBtn.Text = "üíÄ"
toggleBtn.BackgroundColor3 = Color3.fromRGB(30,30,30)
toggleBtn.TextColor3 = Color3.new(1,1,1)
toggleBtn.TextScaled = true
toggleBtn.Draggable = true
toggleBtn.Active = true

local visible = true
toggleBtn.MouseButton1Click:Connect(function()
    visible = not visible
    for _, v in pairs(game.CoreGui:GetChildren()) do
        if v.Name:find("Rayfield") then
            v.Enabled = visible
        end
    end
end)

--// Notify
function Notify(title, text)
   Rayfield:Notify({
       Title = title,
       Content = text,
       Duration = 3,
       Image = 4483362458,
   })
end

--// Tab MAIN
local Main = Window:CreateTab("Main", 4483362458)

Main:CreateSlider({
   Name = "Speed",
   Range = {16, 200},
   Increment = 1,
   CurrentValue = 16,
   Callback = function(val)
       game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
       Notify("Speed", "ƒê√£ ch·ªânh t·ªëc ƒë·ªô: "..val)
   end
})

Main:CreateToggle({
   Name = "NoClip",
   CurrentValue = false,
   Callback = function(state)
       getgenv().noclip = state
       Notify("NoClip", state and "ƒê√£ b·∫≠t NoClip" or "ƒê√£ t·∫Øt NoClip")
       game:GetService("RunService").Stepped:Connect(function()
           if getgenv().noclip then
               for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                   if v:IsA("BasePart") then
                       v.CanCollide = false
                   end
               end
           end
       end)
   end
})

Main:CreateToggle({
   Name = "ESP",
   CurrentValue = false,
   Callback = function(state)
       Notify("ESP", state and "ƒê√£ b·∫≠t ESP" or "ƒê√£ t·∫Øt ESP")
       for _, plr in pairs(game.Players:GetPlayers()) do
           if plr ~= game.Players.LocalPlayer and plr.Character then
               if not plr.Character:FindFirstChild("ESP") then
                   local esp = Instance.new("BillboardGui", plr.Character:WaitForChild("Head"))
                   esp.Name = "ESP"
                   esp.Size = UDim2.new(0,100,0,40)
                   esp.AlwaysOnTop = true
                   local label = Instance.new("TextLabel", esp)
                   label.Size = UDim2.new(1,0,1,0)
                   label.BackgroundTransparency = 1
                   label.TextColor3 = Color3.new(1,0,0)
                   label.Text = plr.Name
               end
           end
       end
   end
})

--// Tab TELEPORT
local Teleport = Window:CreateTab("Teleport", 4483362458)
local savedPoints = {}

Teleport:CreateButton({
   Name = "Save Point",
   Callback = function()
       local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
       table.insert(savedPoints, pos)
       local index = #savedPoints
       Teleport:CreateButton({
           Name = "Point "..index,
           Callback = function()
               game.Players.LocalPlayer.Character:MoveTo(savedPoints[index])
               Notify("Teleport", "ƒê√£ d·ªãch chuy·ªÉn ƒë·∫øn Point "..index)
           end
       })
       Notify("L∆∞u v·ªã tr√≠", "ƒê√£ l∆∞u Point "..index)
   end
})

--// Tab PLAYERS
local Players = Window:CreateTab("Players", 4483362458)
local dropdown

dropdown = Players:CreateDropdown({
   Name = "Ch·ªçn ng∆∞·ªùi ch∆°i",
   Options = {},
   CurrentOption = "",
   Callback = function(name)
       local plr = game.Players:FindFirstChild(name)
       if plr and plr.Character then
           game.Players.LocalPlayer.Character:MoveTo(plr.Character.HumanoidRootPart.Position)
           Notify("Teleport", "ƒê·∫øn: "..name)
       end
   end
})

Players:CreateButton({
   Name = "Load danh s√°ch ng∆∞·ªùi ch∆°i",
   Callback = function()
       local list = {}
       for _,p in pairs(game.Players:GetPlayers()) do
           if p ~= game.Players.LocalPlayer then
               table.insert(list, p.Name)
           end
       end
       dropdown:Refresh(list)
       Notify("Players", "ƒê√£ t·∫£i danh s√°ch!")
   end
})

--// Tab MISC
local Misc = Window:CreateTab("Misc", 4483362458)

Misc:CreateButton({
   Name = "Fly (script tr∆∞·ªõc)",
   Callback = function()
       loadstring(game:HttpGet("https://pastebin.com/raw/zQmRRPW9"))()
       Notify("Fly", "ƒê√£ b·∫≠t Fly")
   end
})
